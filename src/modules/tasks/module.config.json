{
  "id": "tasks",
  "name": "Tasks",
  "version": "1.0.0",
  "description": "Task management module",
  "enabled": true,
  "routes": [
    {
      "path": "/tasks",
      "component": "index",
      "title": "Tasks",
      "requiresAuth": true
    }
  ],
  "api": {
    "basePath": "/api/tasks",
    "endpoints": [
      {
        "method": "GET",
        "path": "",
        "handler": "list",
        "requiresAuth": true
      },
      {
        "method": "POST",
        "path": "",
        "handler": "create",
        "requiresAuth": true
      },
      {
        "method": "GET",
        "path": "/:id",
        "handler": "getById",
        "requiresAuth": true
      },
      {
        "method": "PATCH",
        "path": "/:id",
        "handler": "update",
        "requiresAuth": true
      },
      {
        "method": "DELETE",
        "path": "/:id",
        "handler": "delete",
        "requiresAuth": true
      }
    ]
  },
  "navigation": {
    "label": "Tasks",
    "icon": "CheckSquare",
    "path": "/tasks",
    "order": 4
  },
  "permissions": {
    "create": "tasks:create",
    "read": "tasks:read",
    "update": "tasks:update",
    "delete": "tasks:delete",
    "import": "tasks:import",
    "export": "tasks:export",
    "manage_labels": "tasks:manage_labels",
    "duplicate": "tasks:duplicate",
    "manage": "tasks:*"
  },
  "custom_field": false
}


