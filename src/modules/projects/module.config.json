{
  "id": "projects",
  "name": "Projects",
  "version": "1.0.0",
  "description": "Project tracking module",
  "enabled": true,
  "routes": [
    {
      "path": "/projects",
      "component": "index",
      "title": "Projects",
      "requiresAuth": true
    },
    {
      "path": "/projects/:id",
      "component": "detail",
      "title": "Project Details",
      "requiresAuth": true
    }
  ],
  "api": {
    "basePath": "/api/projects",
    "endpoints": [
      {
        "method": "GET",
        "path": "",
        "handler": "list",
        "requiresAuth": true
      },
      {
        "method": "POST",
        "path": "",
        "handler": "create",
        "requiresAuth": true
      },
      {
        "method": "GET",
        "path": "/:id",
        "handler": "getById",
        "requiresAuth": true
      },
      {
        "method": "PATCH",
        "path": "/:id",
        "handler": "update",
        "requiresAuth": true
      },
      {
        "method": "DELETE",
        "path": "/:id",
        "handler": "delete",
        "requiresAuth": true
      },
      {
        "method": "GET",
        "path": "/:id/tasks",
        "handler": "tasks/list",
        "requiresAuth": true
      },
      {
        "method": "POST",
        "path": "/:id/tasks",
        "handler": "tasks/create",
        "requiresAuth": true
      },
      {
        "method": "PATCH",
        "path": "/:id/tasks/:taskId",
        "handler": "tasks/update",
        "requiresAuth": true
      },
      {
        "method": "DELETE",
        "path": "/:id/tasks/:taskId",
        "handler": "tasks/delete",
        "requiresAuth": true
      },
      {
        "method": "POST",
        "path": "/:id/duplicate",
        "handler": "duplicate",
        "requiresAuth": true
      }
    ]
  },
  "navigation": {
    "label": "Projects",
    "icon": "Briefcase",
    "path": "/projects",
    "order": 3
  },
  "permissions": {
    "create": "projects:create",
    "read": "projects:read",
    "update": "projects:update",
    "delete": "projects:delete",
    "import": "projects:import",
    "export": "projects:export",
    "manage_labels": "projects:manage_labels",
    "duplicate": "projects:duplicate",
    "manage": "projects:*"
  },
  "fields": {
    "enabled": true,
    "configPath": "./config/fields.config.ts"
  },
  "custom_field": false
}


