{
  "id": "notes",
  "name": "Notes",
  "version": "1.0.0",
  "description": "Simple note-taking module",
  "enabled": true,
  "routes": [
    {
      "path": "/notes",
      "component": "index",
      "title": "Notes",
      "requiresAuth": true
    }
  ],
  "api": {
    "basePath": "/api/notes",
    "endpoints": [
      {
        "method": "GET",
        "path": "",
        "handler": "list",
        "requiresAuth": true
      },
      {
        "method": "POST",
        "path": "",
        "handler": "create",
        "requiresAuth": true
      },
      {
        "method": "GET",
        "path": "/:id",
        "handler": "getById",
        "requiresAuth": true
      },
      {
        "method": "PATCH",
        "path": "/:id",
        "handler": "update",
        "requiresAuth": true
      },
      {
        "method": "DELETE",
        "path": "/:id",
        "handler": "delete",
        "requiresAuth": true
      }
    ]
  },
  "fields": {
    "enabled": true,
    "configPath": "./config/fields.config.ts"
  },
  "custom_field": false,
  "navigation": {
    "label": "Notes",
    "icon": "FileText",
    "path": "/notes",
    "order": 5
  },
  "permissions": {
    "create": "notes:create",
    "read": "notes:read",
    "update": "notes:update",
    "delete": "notes:delete",
    "import": "notes:import",
    "export": "notes:export",
    "manage_labels": "notes:manage_labels",
    "duplicate": "notes:duplicate",
    "manage": "notes:*"
  }
}


